{
	"name": "My First Extension",
	"version": "1.0",
	"manifest_version": 2,
	"description": "The first extension that I made.",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
		"http://api.flickr.com/"
	],
	"content_scripts": [
		{
			"matches": ["https://www.baidu.com/*"],
			// "css": ["mystyles.css"],
			"js": ["assets/js/jquery.min.js"]
		}
	],
	"browser_action": {
		"default_title": "pop",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}